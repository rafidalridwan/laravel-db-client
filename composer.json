{
    "name": "RafidAlRidwan/laravel-db-client",
    "description": "Lightweight database client for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "database",
        "client",
        "db-client",
        "database-manager"
    ],
    "authors": [
        {
            "name": "Rafid Al Ridwan",
            "email": "rafidalridwan@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rafid\\DbClient\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rafid\\DbClient\\DbClientServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10|^11"
    },
    "homepage": "https://github.com/RafidAlRidwan/laravel-db-client",
    "support": {
        "source": "https://github.com/RafidAlRidwan/laravel-db-client",
        "issues": "https://github.com/RafidAlRidwan/laravel-db-client/issues"
    }
}
